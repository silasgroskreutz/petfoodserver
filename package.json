{
  "name": "petfoodserver",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "(knex migrate:rollback && knex migrate:latest && knex seed:run) && cross-env DB_ENV=testing jest --verbose --watch",
    "develop": "cross-env DB_ENV=development nodemon index.js",
    "server": "cross-env DB_ENV=production nodemon index.js",
    "reload": "knex migrate:rollback && knex migrate:latest && knex seed:run && yarn develop",
    "start": "node index.js",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "faker": "^4.1.0",
    "helmet": "^3.15.0",
    "heroku": "^7.19.4",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "path": "^0.12.7",
    "sqlite3": "^4.0.6",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
